<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_item_commission"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/green_stroke"
    android:gravity="center_vertical"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/txt_time_history_hh"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:layout_weight="1.3"
        android:background="@color/green_title_table"
        android:gravity="left"
        android:paddingLeft="@dimen/dp1"
        android:paddingTop="@dimen/pading_icon"
        android:paddingRight="@dimen/dp1"
        android:paddingBottom="@dimen/pading_icon"
        android:singleLine="true"

        android:textColor="@color/white"
        android:textSize="@dimen/txt_14sp"
        android:textStyle="italic" />

    <TextView
        android:id="@+id/txt_value_add_history_hh"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:layout_weight="1"
        android:background="@color/green_title_table"
        android:gravity="right"
        android:paddingLeft="@dimen/dp1"
        android:paddingTop="@dimen/pading_icon"
        android:paddingRight="@dimen/dp1"
        android:paddingBottom="@dimen/pading_icon"
        android:singleLine="true"

        android:textColor="@color/white"
        android:textSize="@dimen/txt_14sp" />

    <TextView

        android:id="@+id/txt_value_total_history_hh"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="0.5dp"
        android:layout_weight="1"
        android:background="@color/green_title_table"
        android:gravity="right"
        android:paddingLeft="@dimen/dp1"
        android:paddingTop="@dimen/pading_icon"
        android:paddingRight="@dimen/dp1"
        android:paddingBottom="@dimen/pading_icon"
        android:singleLine="true"

        android:textColor="@color/white"
        android:textSize="@dimen/txt_14sp" />


</LinearLayout>